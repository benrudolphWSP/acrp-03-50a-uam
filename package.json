{
  "name": "acrp-03-50a-uam",
  "version": "1.0.0",
  "description": "Advanced Air Mobility (AAM) Airport Assessment Tool.",
  "keywords": [
    "Netlify",
    "Eleventy",
    "SSG",
    "Tailwind CSS",
    "Webpack"
  ],
  "author": "Ben Rudolph",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "eleventy --watch",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/css/tailwind.css --o _site/include/css/main.css --watch",
    "browsersync": "browser-sync start --server '_site' --files '_site' --port 8080 --no-notify --no-open",
    "clean": "rimraf _site",
    "serve:webpack": "webpack --mode development --output-pathinfo --watch --config webpack.dev.config.js",
    "build:webpack": "webpack --mode=production --config webpack.prod.config.js",
    "build:eleventy": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/css/tailwind.css --o _site/include/css/main.css",
    "build:css": "cross-env NODE_ENV=production postcss src/css/tailwind.css --o _site/include/css/main.css",
    "netlify": "npx netlify-cms-proxy-server",
    "11tydev": "npm-run-all --parallel start serve:webpack netlify",
    "11typrod": "npm-run-all build:eleventy build:webpack build:css",
    "dev": "npm-run-all --parallel css serve:webpack netlify",
    "prod": "npm-run-all build:webpack build:css",
    "build": "npm-run-all build:webpack build:css"
  },
  "main": "main.js",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "highlight.js": "^11.2.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "luxon": "^1.25.0",
    "netlify-cms": "^2.10.140",
    "npm": "^7.21.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "autoprefixer": "^10.1.0",
    "install": "^0.13.0",
    "markdown-it-attrs": "^4.0.0",
    "micromodal": "^0.4.6",
    "modal-video": "^2.4.2",
    "prismjs": "^1.24.1",
    "promise-polyfill": "8.2.0",
    "swiper": "^7.0.4",
    "what-input": "^5.2.10"
  },
  "browserslist": [
    ">0.2%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "Safari >= 13",
    "iOS >= 12"
  ]
}
